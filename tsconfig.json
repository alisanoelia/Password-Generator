{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	],
	"compilerOptions": {
		"module": "ESNext",
		"strict": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			],
			"@layouts/*": [
				"./src/layouts/*"
			],
			"@components/*": [
				"./src/components/*"
			]
		}
	}
}
